{
  "name": "cdnm",
  "version": "1.0.2",
  "description": "CDN Manager",
  "files": [
    "lib"
  ],
  "main": "lib",
  "bin": "lib/cli.js",
  "scripts": {
    "prepare": "babel *.js -d lib",
    "pretest": "eslint *.js",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nickmccurdy/cdnm.git"
  },
  "author": "Nick McCurdy <nick@nickmccurdy.com> (http://nickmccurdy.com/)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/nickmccurdy/cdnm/issues"
  },
  "homepage": "https://github.com/nickmccurdy/cdnm#readme",
  "dependencies": {
    "commander": "^2.11.0",
    "npm-check-updates": "^2.13.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-jest": "^21.2.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "eslint": "^4.8.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.2.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "jest": "^21.2.1"
  },
  "engines": {
    "node": ">=8"
  },
  "babel": {
    "ignore": "*.test.js",
    "plugins": [
      "transform-object-rest-spread"
    ],
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": 4
          }
        }
      ]
    ]
  },
  "eslintConfig": {
    "extends": "standard",
    "overrides": {
      "files": "*.test.js",
      "env": {
        "jest": true
      }
    }
  },
  "jest": {
    "testEnvironment": "node"
  }
}
